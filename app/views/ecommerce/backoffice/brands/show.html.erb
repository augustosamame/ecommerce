<div class="page-header">
  <%= link_to backoffice_brands_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_backoffice_brand_path(@backoffice_brand), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show Brand</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @backoffice_brand.name %></dd>
  <dt>Logo:</dt>
  <dd><%= image_tag @backoffice_brand.logo.medium_200.url.to_s %></dd>
  <dt>Featured:</dt>
  <dd><%= @backoffice_brand.featured %></dd>
  <dt>Display Order:</dt>
  <dd><%= @backoffice_brand.display_order %></dd>

</dl>
