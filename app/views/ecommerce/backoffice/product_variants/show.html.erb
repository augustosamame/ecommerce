<div class="page-header">
  <%= link_to backoffice_product_variants_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_backoffice_product_variant_path(@backoffice_product_variant), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show backoffice_product_variant</h1>
</div>

<dl class="dl-horizontal">
  <dt>Product:</dt>
  <dd><%= @backoffice_product_variant.product %></dd>

  <dt>Variant name:</dt>
  <dd><%= @backoffice_product_variant.variant_name %></dd>

</dl>
