<%= simple_form_for([:backoffice, @backoffice_province]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :province %>
    <%= f.input :district %>
    <%= f.input :priority, label: "Priority (1 first)" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
