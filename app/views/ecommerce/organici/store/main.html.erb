							<li data-target="#myCarousel" data-slide-to="1"></li>
						</ol>

						<!-- Wrapper for slides -->

						<div class="carousel-inner">
							<% if Ecommerce::Slider.where(slider_view: 'Mobile').order(:slider_order).first %>
