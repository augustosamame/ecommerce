								<img src="<%= Ecommerce::Slider.where(slider_view: 'Mobile').order(:slider_order).first.slider_image.url %>">
							</div>
							<% end %>

							<% if Ecommerce::Slider.where(slider_view: 'Mobile').order(:slider_order).second %>
