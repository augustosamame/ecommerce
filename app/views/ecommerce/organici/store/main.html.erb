		<div class="container-fluid dvt_houses_tope">
			<div class="row">
				<!--menu izquierda -->
				<div class="col-md-2 dvt_houses" >
					<ul class="dvt_houses_lista " style="margin-top:30px;">
						<li>
							<div class="visible-xs visible-sm col-xs-12 ">
								<button type="button" class="btn btn-primary btn-lg btn-block dvt_houses_spac_btn">CATEGORY</button>
							</div>
							<div class="visible-xs visible-sm col-xs-12 ">
								<button type="button" class="btn btn-primary btn-lg btn-block">HOUSES</button>
							</div>
						</li>
						<li >
						<% @secondary_menu_categories.each do |category| %>
							<div class="dvt_houses_contenedor hidden-xs hidden-sm" >
								<a href=" <%= products_path(category_id: category.id) %> ">
									<%= image_tag category.image.url, width: 187, height: 49, alt: "", class: "dvt_houses_img" %>
									<span class="dvt_houses_titulos" style="text-align:center;"><%= category.image_popular_homepage_overlay_text %></span>
								</a>
							</div>
						<% end %>
					</li>
					</ul>

				</div>
					<!--fin menu izquierda -->
						<!--slide -->
				<div class="col-sm-10" style="margin-top:30px;" >
					<div id="myCarousel" class="carousel slide" data-ride="carousel">
						  <!-- Indicators -->
						  <ol class="carousel-indicators">
						    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
						    <li data-target="#myCarousel" data-slide-to="1"></li>
						  </ol>

						  <!-- Wrapper for slides -->

						  <div class="carousel-inner">

						  	<div class="dvt_frase"><%= Ecommerce::Control.get_control_value('home_slider_text') %></div>
						    <div class="item active">
						    	<div class="dvt_slide_video"><img src="https://s3.amazonaws.com/devtechperu-expatshop-dev/static/images/home/Sliders/LiveDearestIndianskimmer-small.gif" width="120" height="100"></div>
						      <img src="<%= Ecommerce::Control.find_by(name: 'home_slider_image1').text_value %>" alt="Slider1 Images">
						    </div>

						    <div class="item">
						      <img src="<%= Ecommerce::Control.find_by(name: 'home_slider_image2').text_value %>" alt="Slider2 Image">
						    </div>

								<div class="item">
						      <img src="<%= Ecommerce::Control.find_by(name: 'home_slider_image3').text_value %>" alt="Slider3 Image">
						    </div>


						  <!-- Left and right controls -->
						  <a class="left carousel-control" href="#myCarousel" data-slide="prev">
						    <span class="glyphicon glyphicon-chevron-left"></span>
						    <span class="sr-only">Previous</span>
						  </a>
						  <a class="right carousel-control" href="#myCarousel" data-slide="next">
						    <span class="glyphicon glyphicon-chevron-right"></span>
						    <span class="sr-only">Next</span>
						  </a>
						</div>
					</div>
			</div>
				<!--fin slide -->
		</div>

