								<div class="tp-caption dvt_frase" style=""><%= Ecommerce::Slider.where(slider_view: 'Mobile').order(:slider_order).second.slider_text %></div>
								<img src="<%= Ecommerce::Slider.where(slider_view: 'Mobile').order(:slider_order).second.slider_image.url %>">
							</div>
							<% end %>

							<% if Ecommerce::Slider.where(slider_view: 'Mobile').order(:slider_order).third %>
